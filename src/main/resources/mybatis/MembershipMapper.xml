<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.astro.nautica.Mapper.MembershipMapper">
    <select id="BookmarkList" parameterType="String" resultType="com.astro.nautica.VO.ProductVO">
    	select p.pId, productName, price, ProductImage
    	from product p, bookmarks b 
    	where p.pid = b.pid and userId = #{userId}
    </select>
</mapper>

